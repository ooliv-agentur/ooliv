
{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ooliv.de"
        }
      ],
      "destination": "https://ooliv.de/$1",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/de/:path*",
      "destination": "/:path*",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/de/?",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path*.php",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/author/:path*",
      "destination": "/ueber-uns",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/projekte/:path*",
      "destination": "/referenzen",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/projekte",
      "destination": "/referenzen",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/leistungen/:path*",
      "destination": "/webdesign",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/leistungen",
      "destination": "/webdesign",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/ooliv/:path*",
      "destination": "/ueber-uns",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/ooliv",
      "destination": "/ueber-uns",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/ueber-ooliv/:path*",
      "destination": "/ueber-uns",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/ueber-ooliv",
      "destination": "/ueber-uns",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/case-studies/:path*",
      "destination": "/referenzen",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/case-studies",
      "destination": "/referenzen",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/werbeagentur-frankfurt/:path*",
      "destination": "/werbeagentur-frankfurt",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-wiesbaden/:path*",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-wiesbaden",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-frankfurt/:path*",
      "destination": "/werbeagentur-frankfurt",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-frankfurt",
      "destination": "/werbeagentur-frankfurt",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wordpress-agentur-wiesbaden/:path*",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wordpress-agentur-wiesbaden",
      "destination": "/werbeagentur-wiesbaden",
