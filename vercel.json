
{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ooliv.de"
        }
      ],
      "destination": "https://ooliv.de/$1",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        },
        {
          "key": "Link",
          "value": "<https://ooliv.de>; rel=\"canonical\""
        }
      ]
    }
  ]
}
