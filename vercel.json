
{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ooliv.de"
        }
      ],
      "destination": "https://ooliv.de/$1",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/de/:path*",
      "destination": "/:path*",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/de/?",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path*.php",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/author/:path*",
      "destination": "/ueber-ooliv",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/projekte/:path*",
      "destination": "/case-studies",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/projekte",
      "destination": "/case-studies",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/leistungen/:path*",
      "destination": "/webdesign",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/leistungen",
      "destination": "/webdesign",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/ooliv/:path*",
      "destination": "/ueber-ooliv",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/ooliv",
      "destination": "/ueber-ooliv",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/werbeagentur-frankfurt/:path*",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/werbeagentur-frankfurt",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-wiesbaden/:path*",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-wiesbaden",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-frankfurt/:path*",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/webdesign-agentur-frankfurt",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wordpress-agentur-wiesbaden/:path*",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wordpress-agentur-wiesbaden",
      "destination": "/werbeagentur-wiesbaden",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wordpress-agentur-mainz/:path*",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/wordpress-agentur-mainz",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/kunden/:path*",
      "destination": "/case-studies",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/kunden",
      "destination": "/case-studies",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path(.*)/",
      "destination": "/:path*",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/danke",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Link",
          "value": "<https://ooliv.de/danke>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/en/thank-you",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Link",
          "value": "<https://ooliv.de/en/thank-you>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
