    },
    {
      "source": "/kunden",
      "destination": "/referenzen",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/en/:path*",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/en/?",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path(.*)/",
      "destination": "/:path*",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
