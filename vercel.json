

{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ooliv.de"
        }
      ],
      "destination": "https://ooliv.de/$1",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/de/:path*",
      "destination": "/:path*",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/:path*.php",
      "destination": "/",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/author/:path*",
      "destination": "/ueber-ooliv",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/danke",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Link",
          "value": "<https://ooliv.de/danke>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/en/thank-you",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        },
        {
          "key": "Link",
          "value": "<https://ooliv.de/en/thank-you>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

